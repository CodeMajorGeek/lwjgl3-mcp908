package fr.lwjgl3binding.display;

public class DisplayMode {
	
	private int width;
	private int height;
	
	public DisplayMode(int width, int height) {

		this.width = width;
		this.height = height;
	}

	public int getWidth() {
		return width;
	}
	
	public int getHeight() {
		return height;
	}
	
	//TODO: temp fix value
	public int getFrequency() {
		return 60;
	}
}
